  {
    "Name": "ORZID GMENU ABX COVID-19 ANTIBODIES",
    "contents": [
      {
        "Row": 1,
        "Column": 1,
        "Item": "ORZID GMENU ER/UC ABX GENERAL INFORMATION ED",
        "Text": "Select here for help, legend, consults, alternative antimicrobials and more (ED)",
        "Header": 0
      },
      {
        "Row": 9,
        "Column": 1,
        "Item": "ORZ GTX ABX URL MINNEAPOLIS COVID SHAREPOINT SITE",
        "Text": "[Link] COVID-19 SharePoint Site",
        "Header": 0
      },
      {
        "Row": 7,
        "Column": 1,
        "Item": null,
        "Text": "site for inclusion/exclusion criteria, supportive care and additional ",
        "Header": 0
      },
      {
        "Row": 8,
        "Column": 1,
        "Item": null,
        "Text": "information.",
        "Header": 0
      },
      {
        "Row": 15,
        "Column": 1,
        "Item": null,
        "Text": "_______________________________________________________________________________ ",
        "Header": 1
      },
      {
        "Row": 2,
        "Column": 1,
        "Item": "ORZ GTX ABX ON-CALL SCHEDULE",
        "Text": "[Link] Minneapolis On-Call Schedule-Infectious Disease",
        "Header": 0
      },
      {
        "Row": 6,
        "Column": 1,
        "Item": null,
        "Text": "Refer to the Pharmacologic Treatment Guidance on the COVID-19 SharePoint",
        "Header": 0
      },
      {
        "Row": 11,
        "Column": 1,
        "Item": null,
        "Text": "***COVID-19 therapy is a rapidly evolving field, including the widespread ",
        "Header": 1
      },
      {
        "Row": 12,
        "Column": 1,
        "Item": null,
        "Text": "dissemination of therapies with little or no evidence for benefit. Therapies",
        "Header": 1
      },
      {
        "Row": 13,
        "Column": 1,
        "Item": null,
        "Text": "and dosages not specifically listed here or on the NIH clinical guidelines",
        "Header": 1
      },
      {
        "Row": 14,
        "Column": 1,
        "Item": null,
        "Text": "should be discussed with the infectious diseases service.***",
        "Header": 1
      },
      {
        "Row": 31,
        "Column": 1,
        "Item": "ORZID2 GMENU ABX SOTROVIMAB EUA",
        "Text": "Sotrovimab (EUA) [may be ordered by ED/ID]",
        "Header": 0
      },
      {
        "Row": 23,
        "Column": 1,
        "Item": null,
        "Text": "Outpatients who are Priority 1 for Monoclonal Antibody Treatment: ",
        "Header": 1
      },
      {
        "Row": 24,
        "Column": 1,
        "Item": null,
        "Text": "*Unvaccinated OR partially vaccinated OR vaccinated veterans not expected to",
        "Header": 0
      },
      {
        "Row": 25,
        "Column": 1,
        "Item": null,
        "Text": "mount an immune response <AND>",
        "Header": 0
      },
      {
        "Row": 26,
        "Column": 1,
        "Item": null,
        "Text": "*Tested positive for COVID-19 <AND> ",
        "Header": 0
      },
      {
        "Row": 27,
        "Column": 1,
        "Item": null,
        "Text": "*Within 5 days of (mild to moderate) symptom onset <AND>",
        "Header": 0
      },
      {
        "Row": 30,
        "Column": 1,
        "Item": null,
        "Text": "If patient meets all of the above criteria: ",
        "Header": 0
      },
      {
        "Row": 34,
        "Column": 1,
        "Item": null,
        "Text": "MONOCLONAL ANTIBODY SCREENING SCORE-BIPOC+PREGNANT (MASSBP) ",
        "Header": 1
      },
      {
        "Row": 28,
        "Column": 1,
        "Item": null,
        "Text": "*MASSBP score > 4 (see point system at bottom of menu)",
        "Header": 0
      },
      {
        "Row": 32,
        "Column": 1,
        "Item": null,
        "Text": "____________________________________________________________________________",
        "Header": 1
      },
      {
        "Row": 17,
        "Column": 1,
        "Item": null,
        "Text": "POSSIBLY BENEFICIAL THERAPIES - AVAILABLE BY EMERGENCY USE AUTHORIZATION (EUA)",
        "Header": 1
      },
      { "Row": 44, "Column": 1, "Item": null, "Text": "Total:", "Header": 0 },
      {
        "Row": 46,
        "Column": 1,
        "Item": null,
        "Text": ">>MASSBP equals total of points for each of patient's conditions above",
        "Header": 0
      },
      {
        "Row": 35,
        "Column": 1,
        "Item": null,
        "Text": "o Age greater than 65:2 points",
        "Header": 0
      },
      {
        "Row": 36,
        "Column": 1,
        "Item": null,
        "Text": "o BMI greater than 35:2 points",
        "Header": 0
      },
      {
        "Row": 37,
        "Column": 1,
        "Item": null,
        "Text": "o Diabetes mellitus: 2 points ",
        "Header": 0
      },
      {
        "Row": 38,
        "Column": 1,
        "Item": null,
        "Text": "o Cardiovascular disease over the age of 55: 1 point",
        "Header": 0
      },
      {
        "Row": 39,
        "Column": 1,
        "Item": null,
        "Text": "o Immunocompromised status:4 points ",
        "Header": 0
      },
      {
        "Row": 40,
        "Column": 1,
        "Item": null,
        "Text": "o BIPOC status:2 points ",
        "Header": 0
      },
      {
        "Row": 41,
        "Column": 1,
        "Item": null,
        "Text": "o Pregnancy:4 points",
        "Header": 0
      },
      {
        "Row": 42,
        "Column": 1,
        "Item": null,
        "Text": "o Chronic kidney disease: 3 points",
        "Header": 0
      },
      {
        "Row": 43,
        "Column": 1,
        "Item": null,
        "Text": "____________________________________________________",
        "Header": 0
      },
      {
        "Row": 21,
        "Column": 1,
        "Item": "ORZ GTX ABX COVID-19 MONOCLONAL ANTIBODY SOP",
        "Text": "Monoclonal Antibody Allocation SOP",
        "Header": 0
      },
      {
        "Row": 19,
        "Column": 1,
        "Item": null,
        "Text": "For more information on prioritization of COVID-19 monoclonal antibodies",
        "Header": 0
      },
      {
        "Row": 20,
        "Column": 1,
        "Item": null,
        "Text": "for treatment due to supply constraints:",
        "Header": 0
      },
      {
        "Row": 4,
        "Column": 1,
        "Item": null,
        "Text": "MPLS ED COVID-19 MONOCLONAL ANTIBODY PHARMACOLOGIC RECOMMENDATIONS",
        "Header": 1
      }
    ]
  },

